<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:scrollbars="none">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/textNumberIdTitle"
            style="@style/TextMediumBlack16"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="NIK"
            app:layout_constraintTop_toTopOf="parent" />

        <androidx.appcompat.widget.AppCompatEditText
            android:id="@+id/editTextNumberId"
            style="@style/TextRegularBlack16"
            android:layout_width="match_parent"
            android:layout_height="35dp"
            android:background="@drawable/background_edit_text_credentials"
            android:hint="317500123xxx"
            android:paddingLeft="10dp"
            android:paddingRight="10dp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toBottomOf="@id/textNumberIdTitle"
            app:layout_constraintWidth_percent=".7" />

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/textNameTitle"
            style="@style/TextMediumBlack16"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:text="Name"
            app:layout_constraintTop_toBottomOf="@id/editTextNumberId" />

        <androidx.appcompat.widget.AppCompatEditText
            android:id="@+id/editTextName"
            style="@style/TextRegularBlack16"
            android:layout_width="match_parent"
            android:layout_height="35dp"
            android:background="@drawable/background_edit_text_credentials"
            android:hint="Fakhri Pratama"
            android:paddingLeft="10dp"
            android:paddingRight="10dp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toBottomOf="@id/textNameTitle"
            app:layout_constraintWidth_percent=".7" />

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/textPobTitle"
            style="@style/TextMediumBlack16"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:text="POB"
            app:layout_constraintTop_toBottomOf="@id/editTextName" />

        <androidx.appcompat.widget.AppCompatEditText
            android:id="@+id/editTextPob"
            style="@style/TextRegularBlack16"
            android:layout_width="match_parent"
            android:layout_height="35dp"
            android:background="@drawable/background_edit_text_credentials"
            android:hint="Jakarta"
            android:paddingLeft="10dp"
            android:paddingRight="10dp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toBottomOf="@id/textPobTitle"
            app:layout_constraintWidth_percent=".7" />

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/textDobTitle"
            style="@style/TextMediumBlack16"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:text="DOB"
            app:layout_constraintTop_toBottomOf="@id/editTextPob" />

        <androidx.appcompat.widget.AppCompatEditText
            android:id="@+id/editTextDob"
            style="@style/TextRegularBlack16"
            android:layout_width="match_parent"
            android:layout_height="35dp"
            android:background="@drawable/background_edit_text_credentials"
            android:hint="00-00-0000"
            android:paddingLeft="10dp"
            android:paddingRight="10dp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toBottomOf="@id/textDobTitle"
            app:layout_constraintWidth_percent=".7" />

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/textAddressTitle"
            style="@style/TextMediumBlack16"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:text="Alamat"
            app:layout_constraintTop_toBottomOf="@id/editTextDob" />

        <androidx.appcompat.widget.AppCompatEditText
            android:id="@+id/editTextAddress"
            style="@style/TextRegularBlack16"
            android:layout_width="match_parent"
            android:layout_height="35dp"
            android:background="@drawable/background_edit_text_credentials"
            android:hint="Jakarta"
            android:paddingLeft="10dp"
            android:paddingRight="10dp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toBottomOf="@id/textAddressTitle"
            app:layout_constraintWidth_percent=".7" />

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/textReligionTitle"
            style="@style/TextMediumBlack16"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:text="Agama"
            app:layout_constraintTop_toBottomOf="@id/editTextAddress" />

        <androidx.appcompat.widget.AppCompatEditText
            android:id="@+id/editTextReligion"
            style="@style/TextRegularBlack16"
            android:layout_width="match_parent"
            android:layout_height="35dp"
            android:background="@drawable/background_edit_text_credentials"
            android:hint="Islam"
            android:paddingLeft="10dp"
            android:paddingRight="10dp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toBottomOf="@id/textReligionTitle"
            app:layout_constraintWidth_percent=".7" />

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/textMarriageStateTitle"
            style="@style/TextMediumBlack16"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:text="Marriage Status"
            app:layout_constraintTop_toBottomOf="@id/editTextReligion" />

        <androidx.appcompat.widget.AppCompatEditText
            android:id="@+id/editTextMarriageState"
            style="@style/TextRegularBlack16"
            android:layout_width="match_parent"
            android:layout_height="35dp"
            android:background="@drawable/background_edit_text_credentials"
            android:hint="Belum Kawin"
            android:paddingLeft="10dp"
            android:paddingRight="10dp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toBottomOf="@id/textMarriageStateTitle"
            app:layout_constraintWidth_percent=".7" />

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/textTypeOfWorkTitle"
            style="@style/TextMediumBlack16"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:text="Jenis Pekerjaan"
            app:layout_constraintTop_toBottomOf="@id/editTextMarriageState" />

        <androidx.appcompat.widget.AppCompatEditText
            android:id="@+id/editTextTypeOfWork"
            style="@style/TextRegularBlack16"
            android:layout_width="match_parent"
            android:layout_height="35dp"
            android:background="@drawable/background_edit_text_credentials"
            android:hint="Karyawan Swasta"
            android:paddingLeft="10dp"
            android:paddingRight="10dp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toBottomOf="@id/textTypeOfWorkTitle"
            app:layout_constraintWidth_percent=".7" />

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/textCitizensTitle"
            style="@style/TextMediumBlack16"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:text="Kewarganegaraan"
            app:layout_constraintTop_toBottomOf="@id/editTextTypeOfWork" />

        <androidx.appcompat.widget.AppCompatEditText
            android:id="@+id/editTextCitizens"
            style="@style/TextRegularBlack16"
            android:layout_width="match_parent"
            android:layout_height="35dp"
            android:background="@drawable/background_edit_text_credentials"
            android:hint="WNI"
            android:paddingLeft="10dp"
            android:paddingRight="10dp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toBottomOf="@id/textCitizensTitle"
            app:layout_constraintWidth_percent=".7" />

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/textValidUntilTitle"
            style="@style/TextMediumBlack16"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:text="Berlaku Hingga"
            app:layout_constraintTop_toBottomOf="@id/editTextCitizens" />

        <androidx.appcompat.widget.AppCompatEditText
            android:id="@+id/editTextValidUntil"
            style="@style/TextRegularBlack16"
            android:layout_width="match_parent"
            android:layout_height="35dp"
            android:background="@drawable/background_edit_text_credentials"
            android:hint="Seumur Hidup"
            android:paddingLeft="10dp"
            android:paddingRight="10dp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toBottomOf="@id/textValidUntilTitle"
            app:layout_constraintWidth_percent=".7" />

    </androidx.constraintlayout.widget.ConstraintLayout>

</ScrollView>